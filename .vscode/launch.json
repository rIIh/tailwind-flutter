{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "application",
            "cwd": "apps/sandbox",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
        },
        {
            "name": "application with .env",
            "cwd": "apps/sandbox",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define-from-file",
                ".env"
            ]
        },
        {
            "name": "application (profile mode)",
            "cwd": "apps/sandbox",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "profile"
        },
        {
            "name": "application (release mode)",
            "cwd": "apps/sandbox",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "release"
        }
    ]
}